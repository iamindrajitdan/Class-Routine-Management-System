{
  "name": "class-routine-management-api",
  "version": "1.0.0",
  "description": "Backend API for Class Routine Management System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "ts-node src/database/migrations/run.ts",
    "seed": "ts-node src/database/seeds/run.ts"
  },
  "keywords": [
    "class-routine",
    "timetable",
    "scheduling",
    "academic"
  ],
  "author": "Class Routine Management Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "pg": "^8.10.0",
    "redis": "^4.6.7",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "joi": "^17.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "axios": "^1.6.2",
    "pdfkit": "^0.13.0",
    "csv-writer": "^1.6.0",
    "xlsx": "^0.18.5",
    "nodemailer": "^6.9.7",
    "twilio": "^3.85.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/jest": "^29.5.11",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12"
  }
}
